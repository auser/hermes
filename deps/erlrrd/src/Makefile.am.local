# put whatever (auto)make commands here, they will be included from Makefile.am

DIALYZERFLAGS = -Wno_improper_lists

dist_erlappsrc_DATA = 		\
  $(wildcard *.erl)

dist_erlappinclude_DATA =	\
  $(wildcard *.hrl)

erlappebin_SCRIPTS =					\
  @FW_PACKAGE_NAME@.app					\
  $(patsubst %.erl, %.beam, $(dist_erlappsrc_DATA))

check_DATA =			\
  .dialyzer_ok
